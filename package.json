{
	"name": "primer-shell",
	"version": "2.0.0",
	"description": "Primer Shell is a Command Line Interface (CLI) tool designed to streamline the initialization process of Node.js applications, using carefully crafted templates for optimal project structuring.",
	"author": {
		"name": "Peter Kovacs",
		"email": "peterkov37@gmail.com",
		"url": "https://primershell.com/portfolio"
	},
	"maintainers": [
		"thepeterkovacs"
	],
	"homepage": "https://primershell.com",
	"license": "MIT",
	"files": [
		"build",
		"templates",
		"LICENSE",
		"package.json",
		"pnpm-lock.yaml",
		"README.md"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/thepeterkovacs/primer-shell"
	},
	"engines": {
		"node": ">=20.9.0"
	},
	"packageManager": "pnpm@8.10.0",
	"keywords": [
		"Primer Shell",
		"CLI",
		"TypeScript"
	],
	"type": "module",
	"bin": "build/app.js",
	"scripts": {
		"build": "pnpm exec tsc",
		"watch": "tsc -w",
		"start": "node build/app.js",
		"format": "pnpm prettier . --write"
	},
	"dependencies": {
		"chalk": "^5.3.0",
		"cli-spinners": "^2.9.1",
		"execa": "^8.0.1",
		"figlet": "^1.6.0",
		"fs-extra": "^11.1.1",
		"gradient-string": "^2.0.2",
		"inquirer": "^9.2.11",
		"ora": "^7.0.1"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.2.0",
		"@types/figlet": "^1.5.6",
		"@types/fs-extra": "^11.0.3",
		"@types/gradient-string": "^1.1.2",
		"@types/inquirer": "^9.0.3",
		"typescript": "^5.2.2"
	}
}
